{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/routes/auth.js"
    },
    {
      "src": "/api/trips/(.*)",
      "dest": "/routes/trip.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
} 